{
  "name": "3ddatalab",
  "version": "1.0.0",
  "description": "Website voor 3ddatalab",
  "main": "index.js",
  "repository": "https://github.com/Geodan/website_3ddata.git",
  "author": "Tom van Tilburg <tom.van.tilburg@gmail.com>",
  "license": "MIT",
  "flat": true,
  "scripts": {
    "start": "polymer serve",
    "build": "npm run build:prpl-server && npm run build:static",
    "build:prpl-server": "polymer build --auto-base-path && rm -rf server/build && mv build server/ && renamer --find node_modules --replace node_assets 'server/build/**' && replace node_modules node_assets server/build -r -s",
    "build:static": "polymer build",
    "serve:prpl-server": "prpl-server --root server/build",
    "serve:static": "polymer serve --port 5000 build/es5-bundled",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "mocha test/integration --timeout=10000",
    "test:unit": "polymer test --module-resolution=node --npm -l chrome -l firefox",
    "test:regenerate_screenshots": "mocha test/integration/screenshots-baseline/regenerate.js --timeout=15000"
  },
  "dependencies": {
    "@polymer/app-layout": "^3.0",
    "@polymer/app-route": "^3.0",
    "@polymer/app-storage": "^3.0",
    "@polymer/iron-collapse": "^3.0",
    "@polymer/iron-flex-layout": "^3.0",
    "@polymer/iron-form": "^3.0",
    "@polymer/iron-icon": "^3.0",
    "@polymer/iron-icons": "^3.0",
    "@polymer/iron-iconset-svg": "^3.0",
    "@polymer/iron-media-query": "^3.0",
    "@polymer/iron-overlay-behavior": "^3.0",
    "@polymer/iron-pages": "^3.0",
    "@polymer/iron-selector": "^3.0",
    "@polymer/paper-button": "^3.0",
    "@polymer/paper-card": "^3.0",
    "@polymer/paper-checkbox": "^3.0",
    "@polymer/paper-dialog": "^3.0",
    "@polymer/paper-dialog-scrollable": "^3.0",
    "@polymer/paper-icon-button": "^3.0",
    "@polymer/paper-input": "^3.0",
    "@polymer/paper-item": "^3.0.1",
    "@polymer/paper-listbox": "^3.0",
    "@polymer/paper-spinner": "^3.0",
    "@polymer/paper-tabs": "^3.0.1",
    "@polymer/paper-toggle-button": "^3.0",
    "@polymer/polymer": "^3.0",
    "@webcomponents/webcomponentsjs": "^2.2",
    "lit-element": "^2.1.0",
    "lit-html": "^1.0",
    "pwa-helpers": "^0.9.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "axe-core": "^3.0.0",
    "chai": "^4.1.2",
    "mocha": "^5.1.0",
    "pixelmatch": "^4.0.2",
    "polymer-cli": "^1.7.1",
    "polyserve": "^0.27.0",
    "prpl-server": "^1.1.0",
    "puppeteer": "^1.3.0",
    "renamer": "^0.6.1",
    "replace": "^0.3.0",
    "wct-browser-legacy": "0.0.1-pre.12"
  }
}
